<title>Home</title>
<%- include('./partials/header') %>
<%- include('./partials/nav') %>

<div class="container m-5 d-flex gap-5">
    <form class="w-75 h-100" action="/create-data" method="post">
        <div class="image">
            <label for="text" class="form-label">Property Image</label>
          <input type="text" class="form-control" id="propertyImage" name="image">
        </div>
        <div class="mb-1">
          <label for="text" class="form-label">Owner Name</label>
          <input type="text" class="form-control" id="name" name="name">
        </div>
        <div class="mb-1">
          <label for="property" class="form-label">Property title</label>
          <input type="text" class="form-control" id="property" name="property" >
        </div>
        <div class="mb-1">
          <label for="contact" class="form-label">Contact</label>
          <input type="contact" class="form-control" id="contact" name="contact" >
        </div>
        <div class="mb-1">
          <label for="description" class="form-label">Description</label>
          <input type="text" class="form-control" id="description" name="description">
        </div>
        <div class="mb-1">
            <label for="address" class="form-label">Address</label>
            <input type="text" class="form-control" id="address" name="address" >
          </div>
        <div class="mb-1">
            <label for="price" class="form-label">Price</label>
            <input type="number" class="form-control" id="price" name="price" >
          </div>
          <br>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
</div>

<%- include('./partials/footer') %>